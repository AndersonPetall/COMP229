<div>
  <div class="<%= classTypeInfor %>">
    <hr />
    <p>Current Information:</p>
    <span>name:</span> <%= user.username %><br />
    <span>phone:</span> <%= user.phone %><br />
    <span>email:</span> <%= user.email %><br />
    <span>message:</span> <%= user.message %><br />
  </div>
  <div>
    <form action="/" method="post">
      <fieldset>
        <legend>&nbsp&nbsp</span> <%= activity %>&nbsp</legend>
        Name: <input type="text" name="username" id="name" /><br />
        Email: <input type="text" name="email" id="email" /><br />
        Phone: <input type="text" name="phone" id="phone" /><br />
        Password: <input type="password" name="password" id="password"/><br />
        Message:<br />
        <textarea name="message" rows="3" cols="28" id="message"></textarea><br />
        <button type="submit" formmethod="post" formaction="/Register" class="<%= classTypeRegister %> buttonRegister">Register</button>
        <button type="submit" formmethod="post" formaction="/ListOne/update/<%= user.id %>" class="<%= classTypeUpdate %> buttonUpdate">Update</button>
      </fieldset>
    </form><br>
    <form>
        <button type="submit" formmethod="post" formaction="/ListOne/delete/<%= user.id %>" class="<%= classTypeDelete %> buttonDelete">Delete</button>
        <button class="<%= classTypeCancel %>"><a href="/List">Cancel</a></button>
        
    </form>
  </div>  
</div>
