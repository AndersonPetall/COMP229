<!DOCTYPE html>
<html lang="en">
  <%-include("./partials/head")%>
  <body>
    <%-include("./partials/header")%>
    <table
      width="800"
      border="0"
      align="center"
      cellpadding="0"
      cellspacing="0"
      class="main"
    >
      <tr>
        <td></td>
      </tr>
      <tr>
        <td>
          <table width="760" border="0" cellspacing="10" cellpadding="0">
            <tr>
              <td><h2>Login View Page</h2></td>
            </tr>
            <tr>
              <td rowspan="3">
                <form action="/login" method="post">
                  <fieldset>
                    <legend>&nbsp&nbspLogin Form&nbsp&nbsp</legend>
                    username:
                    <input type="text" name="username" id="nameLogin" /><br />
                    password:
                    <input
                      type="password"
                      name="password"
                      id="passwordLogin"
                    /><br /><br />
                    <input type="submit" value="submit" id="submitBtnLogin" />
                    <input type="reset" value="reset" />
                  </fieldset>
                </form>
              </td>
            </tr>
          </table>
        </td>
      </tr>
      <tr>
        <td>&nbsp;</td>
      </tr>
    </table>
    <%-include("./partials/footer")%>
  </body>
</html>
